{
  "name": "mqs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:prod": "npm run rmdocs && webpack --colors --config webpack/webpack.config.js --env=prod",
    "build:analyze": "npm run rmdocs && webpack --colors --config webpack/webpack.config.js --env=analyze",
    "rmdocs": "rm -rf docs",
    "server:dev": "forever forever/dev.json",
    "server:prod": "forever forever/prod.json",
    "webpack:dev": "webpack-dev-server --colors --watch --config webpack/webpack.config.js --env=dev",
    "webpack:prod": "webpack-dev-server --colors --config webpack/webpack.config.js --env=prod",
    "start": "npm run rmdocs && run-p webpack:prod server:prod",
    "start:dev": "npm run rmdocs && run-p webpack:dev server:dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "~0.8.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "forever": "^0.15.3",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.4",
    "worker-loader": "^1.1.0"
  }
}
